<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace必须指向Dao接口 -->
<mapper namespace="cn.com.chanyue.dao.commom.ErrorDao">
	<select id="queryError" parameterType="String" resultType="Error">
		select *
		from
		tb_chanyue_error_remark t where
		t.code = #{code}
	</select>

</mapper> 
